<SessionData xmlns="http://www.qut.com/middleware/SessionDataSchema">
	<Identity>
		<Attribute Identifier="username" Delimiter="|" Type="String" Singleton="true">
			<Handler LocalIdentifier="uid" Name="LDAPHandler"/>
		</Attribute>
		<Attribute Identifier="mail" Delimiter="," Type="String" Singleton="false">
			<Handler LocalIdentifier="mail" Name="LDAPHandler"/>
		</Attribute>
	</Identity>
</SessionData>